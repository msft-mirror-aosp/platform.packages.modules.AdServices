    <!-- Text for the title of the header. [CHAR LIMIT=NONE] -->
    <string name="notificationUI_header_title">You\'re helping Android restrict cross-app tracking</string>

    <!-- Text for the subtitle of the header. [CHAR LIMIT=NONE] -->
    <string name="notificationUI_header_subtitle">Android is testing new features that will restrict cross-app tracking by advertisers</string>

    <!-- Text for container1 title. [CHAR LIMIT=NONE] -->
    <string name="notificationUI_container1_title">What’s cross-app tracking?</string>

    <!-- Text for container1 body text. [CHAR LIMIT=NONE] -->
    <string name="notificationUI_container1_body_text">Today, data about your app usage can easily be associated with your device, then shared across apps for advertising purposes.</string>

    <!-- Text for container2 title. [CHAR LIMIT=NONE] -->
    <string name="notificationUI_container2_title">What Android is doing</string>

    <!-- Text for container2 body text. [CHAR LIMIT=NONE] -->
    <string name="notificationUI_container2_body_text">Android is testing a new Privacy Sandbox that securely stores and shares advertising data — without identifying you or your device. \n\nThis new system helps prevent apps from passing data directly to other apps, but still allows them to show you ads you might like.</string>

    <!-- Text for container2 control text. [CHAR LIMIT=NONE] -->
    <string name="notificationUI_container2_control_text">How it works</string>

    <!-- Text for container3 title. [CHAR LIMIT=NONE] -->
