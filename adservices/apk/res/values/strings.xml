    <!-- Summary of the apps preference. [CHAR LIMIT=NONE] -->
    <string name="settingsUI_apps_summary">Manage your apps that generate specific interests</string>

    <!-- Text for the title for the Topics fragment. [CHAR LIMIT=NONE] -->
    <string name="settingsUI_topics_fragment_title">Manage your general interests for ads</string>

