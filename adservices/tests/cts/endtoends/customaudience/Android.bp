android_test {
    name: "CtsAdServicesCustomAudienceTests",
    team: "trendy_team_android_rubidium",
    srcs: [
        "src/**/*.java",
    ],
    static_libs: [
        "androidx.concurrent_concurrent-futures",
        "compatibility-device-util-axt",
        "truth",
        "adservices-clients",
        "adservices-cts-assets",
        "adservices-test-fixtures",
        "adservices-test-utility",
        "adservices-cts-test-utility",
        "mockwebserver",
    ],
    libs: [
        "android.test.base.stubs.system",
        "framework-adservices-lib",
        "framework-sdksandbox.impl",
    ],
    test_suites: [
        "general-tests",
        "mts-adservices",
        "mcts-adservices",
    ],
    data: [
        // Sample apps will be installed and uninstalled in the test.
        ":CtsSampleCustomAudienceAppHighBid",
        ":CtsSampleCustomAudienceAppLowBid",
    ],
    sdk_version: "module_current",
    min_sdk_version: "Tiramisu",
    lint: {
        extra_check_modules: ["AdServicesTestLintChecker"],
        test: false, // TODO(b/343741206): remove when checks will run on android_test
    },
}

