android_test {
    name: "AdServicesJetpackTests-beta01",
    srcs: [
        "src/**/*.java",
    ],
    exclude_srcs: [
        // jetpack beta01 release has broken AdTechIdentifier toString, causing failures in ad selection
        "src/com/android/adservices/protectedaudience/FledgeCtsDebuggableJetpackTest.java",
    ],
    sdk_version: "module_current",
    static_libs: [
        "adservices-clients",
        "androidx.privacysandbox.ads_ads-adservices-beta01",
        "androidx.privacysandbox.ads_ads-adservices-java-beta01",
        "androidx.concurrent_concurrent-futures",
        "androidx.test.rules",
        "compatibility-device-util-axt",
        "kotlinx-coroutines-android",
        "mockito-target-extended-minus-junit4",
        "truth",
        "framework-adservices.impl",
        "framework-sdksandbox.impl",
        "adservices-test-fixtures",
        "adservices-test-utility",
    ],
    libs: [
        "android.test.base",
    ],
    test_suites: [
        "general-tests",
    ],
    test_config: "AndroidTest-beta01.xml",
}

