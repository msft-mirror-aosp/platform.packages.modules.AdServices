// Java classes that are used by both device-side and host-side test libraries
// NOTE: cannot create a proper "library", as it would be "java_library" on
// device-side and "java_library_host" on host-side. Hence, projects including
// filegroup should add the following dependencies
//     "error_prone_annotations", // @FormatMethod / @FormatString
//     "junit",
//   - device:
//      "guava", // @VisibleForTesting
//   - host
//      "guava-android-host", // @VisibleForTesting
filegroup {
    name: "adservices-shared-sideless-testing-sources",
    srcs: [
        "java/com/android/adservices/shared/testing/**/*.java",
    ],
    visibility: [
        "//packages/modules/AdServices/shared/libraries:__subpackages__",
    ],
}
